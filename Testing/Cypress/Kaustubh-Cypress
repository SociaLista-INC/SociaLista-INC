describe("On Page init", ()=> {
    beforeEach(() => cy.visit("http://localhost:3000"))

    it("cy.get() - query DOM elements", () => {
        cy.get(".slogan").should("contain","Be Simple")
    });
    it("cy.get() - query DOM elements", () => {
        cy.get(".alogan").should("contain","Be Social")
    });
    it("cy.get() - query DOM elements", () => {
        cy.get(".our-info").should("contain","Abdul Alhajkanjo")
    });
    it("cy.get() - query DOM elements", () => {
        cy.get(".our-info").should("contain","Abhishek Duggal")
    });
    it("cy.get() - query DOM elements", () => {
        cy.get(".our-info").should("contain","Kaustubh Deshpande")
    });
    it("find LinkedIn button", () => {
        cy.visit("http://localhost:3000");
        cy.get("Linkedin-button")
    })
})
